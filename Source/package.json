{
  "name": "tiddlywiki-blc-plugin",
  "version": "0.1.0",
  "description": "A TiddlyWiki plugin for kanban like organization in the style of Trello using boards, lists, and cards.",
  "keywords": [
    "tiddlywiki",
    "tiddlywiki5",
    "wiki",
    "kanban",
    "trello",
    "personal",
    "knowledgebase",
    "organization",
    "nonlinear",
    "board",
    "list",
    "card",
    "whiteboard",
    "sticky note"
  ],
  "author": {
    "name": "Reid Gould",
    "email": "reidgould@gmail.com",
    "url": "reidgould.com"
  },
  "contributors": [
    {
      "name": "Reid Gould",
      "email": "reidgould@gmail.com",
      "url": "reidgould.com"
    }
  ],
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/reidgould/tiddlywiki-blc-plugin"
  },
  "license": "SEE LICENSE IN license.txt",
  "engines": {
    "node": ">= 6.11.0",
    "npm": ">= 3.0.0"
  },
  "main": "",
  "scripts": {
    "tw-build-blc": "tiddlywiki ./editions/blc-plugin --output ../Build/Plugin --build blc-plugin --output ../Build/EmptyWithBlc --build empty-with-blc --output . --build meta",
    "tw-build-blc-plugin":     "tiddlywiki ./editions/blc-plugin     --output ../Build/Plugin       --build blc-plugin",
    "tw-build-empty-with-blc": "tiddlywiki ./editions/blc-plugin     --output ../Build/EmptyWithBlc --build empty-with-blc",
    "tw-build-meta":           "tiddlywiki ./editions/blc-plugin     --output .                     --build meta",
    "tw-build-blc-user-guide": "tiddlywiki ./editions/blc-user-guide --output ../Build/UserGuide    --build blc-user-guide"
  },
  "dependencies": {
  },
  "devDependencies": {
    "tiddlywiki": "^5.1.14"
  },
  "bundledDependencies": []
}
